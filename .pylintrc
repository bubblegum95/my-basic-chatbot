[MASTER]
ignore=venv
jobs=1
load-plugins=pylint.extensions.docparams

[MESSAGES CONTROL]
disable=
    missing-docstring,
    invalid-name,
    too-few-public-methods,
    too-many-arguments,
    too-many-locals,
    too-many-instance-attributes,
    duplicate-code

[FORMAT]
max-line-length=100
indent-string='  '  # 들여쓰기

[DESIGN]
max-args=6
max-locals=15
max-attributes=10

[REPORTS]
output-format=colorized
reports=no
score=yes
